<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="custom_switch_button">
        <attr name="android:fontFamily"/>
        <attr format="dimension" name="strokeWidth"/>
        <attr format="dimension" name="android:textSize"/>
        <attr format="boolean" name="android:checked"/>
        <attr format="string" name="textOn"/>
        <attr format="string" name="textOff"/>
        <attr format="boolean" name="showText"/>
        <attr format="enum" name="labelPosition">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
        <attr format="integer" name="speed"/>
        <attr format="color" name="strokeColorOnSwitchOn"/>
        <attr format="color" name="strokeColorOnSwitchOff"/>
        <attr format="color" name="thumbColorOnSwitchOn"/>
        <attr format="color" name="thumbColorOnSwitchOff"/>
        <attr format="color" name="backgroundColorOnSwitchOn"/>
        <attr format="color" name="backgroundColorOnSwitchOff"/>
        <attr format="color" name="textColorOnSwitchOn"/>
        <attr format="color" name="textColorOnSwitchOff"/>
        <attr format="color" name="themeColor"/>
    </declare-styleable>
</resources>